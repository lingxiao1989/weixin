<import src="../ratings/likes-template.wxml" />
<template name="teamTemplate">
  <view class="weui-cell">
      <view class="weui-cell__hd" style="position: relative;margin-right: 10px;">
          <image src="{{teamImage}}" style="width: 50px; height: 50px; display: block;border-radius:25px;" bindtap="clickImage"/>  
      </view>  
      <view class="weui-cell__bd">
        <text url="../login/login" >{{teamName}}</text>  
        <view style="font-size: 13px;color: #888888;">{{teamBrief}}</view>
        <template is="likesTemplate" data="{{ratings:[1, 1, 1, 1, 1], score: 5}}" />
      </view>
  </view>
</template>